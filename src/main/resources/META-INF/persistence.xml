<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
  <persistence-unit name="vertex.schema" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>kernel.runtime.Session</class>
    <class>kernel.runtime.Variable</class>
    <class>kernel.runtime.Member</class>
    <class>kernel.runtime.Group</class>
    <class>kernel.runtime.Permission</class>
    <class>kernel.runtime.Authentication</class>
  </persistence-unit>
</persistence>